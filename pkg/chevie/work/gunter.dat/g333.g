 [1/3*i3*zb*x^4*p6a*p3b^3,
  -1/3*i3*z*x^4*p6b*p3a^3,
  -1/3*i3*x^4*p1^3*p2],

 [x^9]],

fakdeg:=[
 [1],
 [2*x^4 + x, x^5 + 2*x^2],
 [x^6 + x^3],
 [x^6 + x^3],
 [x^6 + x^3],
 [x^6 + x^3],
 [2*x^7 + x^4, x^8 + 2*x^5 ],
 [x^9]],

foumat:=[
 [[1]],
 1/3*i3*[[-z,zb,1],[zb,-z,-1],[1,-1,-1]],
 [[1]],
 [[1]],
 [[1]],
 [[1]],
 1/3*i3*[[zb,-z,-1],[-z,zb,1],[-1,1,1]],
 [[1]]
],
frob:=[
 [1],
 [1,1,z],
 [1],
 [1],
 [1],
 [1],
 [1,1,zb],
 [1]]
);

