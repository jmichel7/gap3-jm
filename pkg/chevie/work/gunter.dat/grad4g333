q:=X(Cyclotomics);q.name:="q";
ff:=function(u,ind)local i,f,j;
f:=List(u,x->ind);
for i in [1..Length(u)] do 
  for j in [1..Length(u)] do
     if j<>i then f[i]:=f[i]*u[j]/(u[i]-u[j]); fi;
  od;
od;
return f;
end;

ind:=(q^6-1)*(q^6+1)/(q^2-1)/(q^2+1);
u:=[1,E(3)*q^4,E(3)^2*q^4];      # d = 1

u:=[E(3),E(3)^2,q^4];      # d = 1
#----------------------------------------------------------

ind:=(q^6-1)*(q^6+1)/(q^2-1)/(q-E(4));
u:=[E(4),E(3)*q,E(3)^2*q,E(3)*E(4)*q^2,E(3)^2*E(4)*q^2,q^3];      # d = 4

#----------------------------------------------------------

