##################################
##  Unipotente  Charaktergrade  ##
##################################
grad:=[1,

1/3*ER(-3)*E(3)^2*q*(q-E(3)^2)*(q+E(3)^2)*(q^2+E(3)),
-1/3*ER(-3)*E(3)*q*(q-E(3))*(q+E(3))*(q^2+E(3)^2),
1/3*q*(q-1)*(q+1)*(q^2+1),

1/3*E(4)*ER(-3)*E(3)^2*q^4*(q-E(3)^2)*(q+E(3)^2)*(q^2+E(3)),
-1/3*E(4)*ER(-3)*E(3)*q^4*(q-E(3))*(q+E(3))*(q^2+E(3)^2),
1/3*E(4)*q^4*(q-1)*(q+1)*(q^2+1),

E(4)*q^9];


