  -(1/4-1/4*I)*x^2*p1*p2*p3*p6*p12*(x^2-I)],

 [1/2*x^3*p4^2*p8*p12,
  1/2*x^3*p4^2*p8*p12,
  1/2*I*x^3*p1^2*p2^2*p3*p6*p8,
  1/2*I*x^3*p1^2*p2^2*p3*p6*p8],

 [-1/12*x^6*p3*p6*p8*(x^2+I*x-1)*(x+I)^2,
  -1/12*x^6*p3*p6*p8*(x^2-I*x-1)*(x-I)^2,
  1/4*x^6*p4*p6*p8*p12,
  -1/4*I*x^6*p2*p4*p6*p12*(x^2+I)*(x+I),
  1/4*I*x^6*p2*p4*p6*p12*(x^2-I)*(x-I),
  1/12*x^6*p2^2*p3*p8*p12,
  1/4*x^6*p3*p4*p8*p12,
  1/4*I*x^6*p1*p3*p4*p12*(x^2-I)*(x+I), ##
  1/4*I*x^6*p1*p3*p4*p12*(x^2+I)*(x-I),
  1/4*x^6*p1*p2*p3*p6*p8*(x^2+I*x-1), ##
  -1/4*x^6*p1*p2*p3*p6*p8*(x^2-I*x-1),
  1/4*I*x^6*p1*p2^2*p3*p6*(x^2+I*x-1)*(x^2-I)*(x-I), ##
  1/4*I*x^6*p1*p2^2*p3*p6*(x^2-I*x-1)*(x^2+I)*(x+I),
  1/12*x^6*p1^2*p6*p8*p12,
  -1/4*I*x^6*p1^2*p2*p3*p6*(x^2+I*x-1)*(x^2+I)*(x-I),
  -1/4*I*x^6*p1^2*p2*p3*p6*(x^2-I*x-1)*(x^2-I)*(x+I),
  1/3*x^6*p1^2*p2^2*p4^2*p8,
  1/3*x^6*p1^2*p2^2*p4^2*p8]],
fakdeg:=[
 [1],
 [x*(1+x^4),
  x^4*(1+x^4),
  x^7*(1+x^4)],
 [x^2*(1+x^4+x^8),
  x^4*(1+x^4+x^8),
  x^6*(1+x^4+x^8)],
 [x^3*(1+x^4+x^8+x^12),
  x^5*(1+2*x^4+x^8)],
 [x^6,
  x^18,
  x^12,
  x^7*(1+x^4),
  x^13*(1+x^4),
  x^10*(1+x^4),
  x^8*(1+x^4+x^8)]],
foumat:=[
 [[1]],
 [[1-I,2,1+I,1+I,2*I,I-1],
  [2,0,2,-2,0,2],
  [1+I,2,1-I,1-I,-2*I,-1-I],
  [1+I,-2,1-I,1-I,2*I,-1-I],
