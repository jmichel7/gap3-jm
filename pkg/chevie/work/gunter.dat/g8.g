###################################
##  Unipotente Grade  G8         ##
###################################
x:=X(Cyclotomics); x.name:="q";
#x:=X(NF([E(4)])); x.name:="x";
## pols:
p1:=x-1; p2:=x+1; p3:=x^2+x+1; p4:=x^2+1; p6:=x^2-x+1;
p8:=x^4+1; p12:=x^4-x^2+1;
## irrat:
I:=E(4);

G8:=rec(
unpdeg:=[
 [1],

 [(1/4-1/4*I)*x*p2*p6*p8*(x^2+I*x-1)*(x-I),
  1/2*x*p4*p8*p12,
  (1/4+1/4*I)*x*p2*p6*p8*(x^2-I*x-1)*(x+I),
  (1/4+1/4*I)*x*p1*p3*p8*(x^2+I*x-1)*(x-I),
  1/2*I*x*p1*p2*p3*p6*p8,
  -(1/4-1/4*I)*x*p1*p3*p8*(x^2-I*x-1)*(x+I)],

 [(1/4-1/4*I)*x^2*p3*p4*p6*p12*(x^2+I),
  1/2*x^2*p3*p6*p8*p12,
  (1/4+1/4*I)*x^2*p3*p4*p6*p12*(x^2-I),
